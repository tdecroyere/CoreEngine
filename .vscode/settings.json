{
    "files.exclude": {
        "**/bin": true,
        "**/obj": true,
        "**/CompilationUnit.cpp": true,
        "src/CoreEngine/CoreEngine.SourceGenerators": true,
        "samples/**/CoreEngine.SourceGenerators": true,
        "tests/**/CoreEngine.SourceGenerators": true
    },
    "files.associations": {
        "*.cescene": "jsonc",
        "*.pipeline": "jsonc",
        "*.idl": "cpp",
        "xstring": "cpp",
        "xlocale": "cpp",
        "xutility": "cpp"
    },
    "omnisharp.enableRoslynAnalyzers": true,
    "csharp.referencesCodeLens.enabled": false
}